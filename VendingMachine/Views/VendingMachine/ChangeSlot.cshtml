<div class="panel-group" ng-show="change.IsChangeExist">
    <div class="panel panel-default">
        <div class="panel-heading">Ваша сдача:</div>
        <div class="panel-body">
            <div class="row">
                <div ng-repeat="changeItem in change.Change">
                    <div class="col-xs-1-10">
                        <img width="40" height="40" src='@Url.Action("Show", "Image")?id={{changeItem.PictureId}}' />
                    </div>
                </div>
            </div>
        </div>
        <div class="panel-footer">
            <a class="btn btn-lg btn-block btn-success" ng-click="clearChange()">
                Забрать сдачу
                <span class="glyphicon glyphicon-hand-right" width="30" aria-hidden="true"></span>
            </a>
        </div>
    </div>
</div>